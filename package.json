{
    "name": "noj_desktop",
    "productName": "noj_desktop",
    "version": "1.0.0",
    "description": "NOJ Desktop is a contest optimized version supporting Windows, Mac and Linux.",
    "main": "src/index.js",
    "scripts": {
        "start": "electron-forge start",
        "debug": "electron-forge start --inspect-electron",
        "startWin": "chcp 65001 && electron-forge start",
        "debugWin": "chcp 65001 && electron-forge start --inspect-electron",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "postinstall": "electron-builder install-app-deps",
        "lint": "echo \"No linting configured\""
    },
    "keywords": [
        "noj",
        "cross-platform",
        "electron"
    ],
    "engines": {
        "node": "12.x"
    },
    "author": {
        "name": "Youjie Zhang",
        "email": "zsgsdesign@gmail.com"
    },
    "homepage": "https://github.com/NJUPTAAA/NOJ_Desktop",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git@github.com:NJUPTAAA/NOJ_Desktop.git"
    },
    "bugs": {
        "url": "https://github.com/NJUPTAAA/NOJ_Desktop/issues"
    },
    "build": {
        "appId": "com.onlinejudge.noj.desktop",
        "productName": "NOJ Desktop",
        "copyright": "Copyright Â© 2020 Fangtang Zhixing Network Technology(Nanjing) Co,Ltd.",
        "artifactName": "${productName}-${version}-${platform}.${ext}",
        "publish": [
            {
                "provider": "github",
                "releaseType": "release"
            }
        ],
        "directories": {
            "output": "release"
        },
        "asar": true,
        "win": {
            "icon": "assets/icon.ico",
            "target": [
                "nsis",
                "zip"
            ],
            "publisherName": "Fangtang Zhixing Network Technology(Nanjing) Co,Ltd."
        },
        "nsis": {
            "oneClick": false,
            "perMachine": false,
            "createDesktopShortcut": false,
            "createStartMenuShortcut": true,
            "installerIcon": "assets/installerIcon.ico",
            "artifactName": "${productName}-${version}-installer.${ext}"
        }
    },
    "dependencies": {
        "animate.css": "^3.7.2",
        "bootstrap-material-design": "^4.1.2",
        "compare-versions": "^3.6.0",
        "dompurify": "^2.0.11",
        "electron-squirrel-startup": "^1.0.0",
        "electron-store": "^7.0.2",
        "fileicon-svg": "^1.0.0",
        "highlightjs": "^9.16.2",
        "jquery": "^3.5.1",
        "marked": "^1.0.0",
        "mathjax": "^2.7.8",
        "monaco-editor": "^0.20.0",
        "node-notifier": "^7.0.0",
        "popper.js": "^1.15.0",
        "request": "^2.88.2",
        "simplemde": "^1.11.2",
        "snackbarjs": "^1.1.0",
        "underscore": "^1.10.2"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.51",
        "@electron-forge/maker-deb": "^6.0.0-beta.51",
        "@electron-forge/maker-rpm": "^6.0.0-beta.51",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.51",
        "@electron-forge/maker-zip": "^6.0.0-beta.51",
        "electron": "8.2.5",
        "electron-builder": "^22.6.0"
    }
}
